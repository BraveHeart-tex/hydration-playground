    "@types/react-dom": "19.0.4",
    "eslint": "^9",
    "eslint-config-next": "15.2.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8",
    "prettier": "^3.6.2",
